%.c	Makefile	/^%.c:$/;"	t
CC	Makefile	/^CC      := gcc-11$/;"	m
CCFLAGS	Makefile	/^CCFLAGS := -Werror -Wall -Wextra -Wpedantic -Wshadow \\$/;"	m
DEBUG	src/main.c	/^#define DEBUG$/;"	d	file:
FLAGS	src/flags.h	/^#define FLAGS$/;"	d
Flags	src/flags.c	/^struct flags Flags;$/;"	v	typeref:struct:flags
LD	Makefile	/^LD      := $(CC)$/;"	m
LDFLAGS	Makefile	/^LDFLAGS := $(CCFLAGS)$/;"	m
LOG	src/macros.h	/^#define LOG(/;"	d
MACROS	src/macros.h	/^#define MACROS$/;"	d
OBJECTS	Makefile	/^OBJECTS := $(SOURCES:src\/%.c=obj\/%.o)$/;"	m
SOURCES	Makefile	/^SOURCES := $(wildcard src\/*.c)$/;"	m
all	Makefile	/^all: $(OBJECTS)$/;"	t
all	man/Makefile	/^all:$/;"	t
btoc	src/main.c	/^const char * btoc(bool tested) {$/;"	f	typeref:typename:const char *
clean	Makefile	/^clean:$/;"	t
clean	man/Makefile	/^clean:$/;"	t
flags	src/flags.h	/^struct flags {$/;"	s
main	src/main.c	/^int main(int argc, char **argv) {$/;"	f	typeref:typename:int
man	Makefile	/^man:$/;"	t
obj/%.o	Makefile	/^obj\/%.o: src\/%.c$/;"	t
parse_flag_long	src/flags.c	/^void parse_flag_long(char flag[]) {$/;"	f	typeref:typename:void
parse_flag_short	src/flags.c	/^void parse_flag_short(char flag) {$/;"	f	typeref:typename:void
set_default_flags	src/flags.c	/^void set_default_flags(void) {$/;"	f	typeref:typename:void
verbose	src/flags.h	/^	bool verbose;$/;"	m	struct:flags	typeref:typename:bool
